{
  "manifest_version": 3,
  "name": "Simple New Tab Redirect",
  "version": "1.0.0",
  "description": "Sets https://home-new-tab.vercel.app/ as your new tab page.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "host_permissions": [
    "https://home-new-tab.vercel.app/*"
  ],
  "background": {
    "service_worker": "bg.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://home-new-tab.vercel.app/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}